#!/usr/bin/env python3
from pygrammalecte import grammalecte_text

texte_bidon = """\
Coucou, je veut du fromage.
Je sais coder en VHDL.
Le VHDL est est compliquer.
"""


if __name__ == '__main__':
    for message in grammalecte_text(texte_bidon):
        #print(dir(message))
        print(message.message)
